name: beanstalk_v1
description: "A new Flutter project."
publish_to: 'none'
version: 0.1.0

environment:
  sdk: ^3.8.1

dependencies:
  connectivity_plus: ^6.1.4
  file_picker: ^10.2.0
  flutter:
    sdk: flutter
  flutter_animate: ^4.5.2
  flutter_colorpicker: ^1.1.0
  flutter_displaymode: ^0.6.0
  fluttertoast: ^8.2.12
  google_generative_ai:
    git:
      url: https://github.com/ComsIndeed/generative-ai-dart.git
      path: pkgs/google_generative_ai
  image_picker: ^1.1.2
  markdown_widget: ^2.3.2+8
  mime: ^2.0.0
  moment_dart: ^5.2.3
  permission_handler: ^12.0.1
  provider: ^6.1.5
  shared_preferences: ^2.5.3
  uuid: ^4.5.1

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^5.0.0

flutter:
  uses-material-design: true
  assets:
    - assets/icon.png
  fonts:
    - family: Kurale
      fonts:
        - asset: assets/fonts/Kurale-Regular.ttf
